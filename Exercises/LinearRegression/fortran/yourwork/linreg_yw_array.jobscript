#!/bin/bash
#SBATCH --job-name="linreg_yw"
#SBATCH --output="linreg_yw%a-%j.out"
#SBATCH --partition=trial
#SBATCH --nodes=2
#SBATCH --ntasks-per-node=16
#SBATCH --cpus-per-task=1
#SBATCH --array=1,2,4,8,16,32
#SBATCH --mem-per-cpu=5000M
#SBATCH --account=RFST
#SBATCH --export=ALL
#SBATCH -t 00:05:00

module load intel

echo "Starting Linear Regression with ${SLURM_ARRAY_TASK_ID} MPI processes" 
time mpirun -n ${SLURM_ARRAY_TASK_ID} ./linreg_yourwork.exe
wait
echo ""
